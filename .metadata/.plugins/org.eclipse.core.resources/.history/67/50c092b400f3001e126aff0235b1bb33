package new01;

public class New01 {

	public static void main(String[] args) {
		/* 홍길동 주민등록번호 881122-1234567이다
		 * 출력형태 -> 생년월일 : 881122, 성별 : 남
		 * 성별이 남인 경우는 1또는 3
		 * */
		
		String birthFirst = "881122";
		String birthLast = "1234567";
		
		if((birthLast.substring(0))=="1"||(birthLast.substring(0))=="3") {
			System.out.println("생년월일 : "+birthFirst+", 성별 : 남");
		}else {
			System.out.println("생년월일 : "+birthFirst+", 성별 : 여");
		}
		

	}

}
